<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="606.0" prefWidth="1002.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.spring.Controller">
   <children>
      <Label text="بيانات الجهاز" />
      <TextArea fx:id="textInfo" nodeOrientation="LEFT_TO_RIGHT" prefHeight="465.0" prefWidth="706.0" text="بينات الجهاز للعميل:&#10;(اكتب هنا بيانات العميل مثل الاسم واللقب)&#10;************************************************************************************************" VBox.vgrow="ALWAYS" />
      <ButtonBar VBox.vgrow="NEVER">
         <buttons>
            <Button mnemonicParsing="false" onAction="#save" text="احفظ" />
            <Button mnemonicParsing="false" onAction="#print" text="اطبع" />
            <Button mnemonicParsing="false" onAction="#getInfo" text="أحضر البيانات" />
            <Button mnemonicParsing="false" onAction="#exit" text="أغلق" />
         </buttons>
      </ButtonBar>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
